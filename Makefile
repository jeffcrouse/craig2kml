	@echo "   debug"
	@echo "   release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   craig2kml"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"
