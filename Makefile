ifndef config
  config=debug
endif
export config

PROJECTS := craig2kml

.PHONY: all clean help $(PROJECTS)

all: $(PROJECTS)

craig2kml: 
	@echo "==== Building craig2kml ($(config)) ===="
	@${MAKE} --no-print-directory -C . -f craig2kml.make

clean:
	@${MAKE} --no-print-directory -C . -f craig2kml.make clean

help:
